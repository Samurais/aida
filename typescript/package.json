{
  "name": "aida",
  "version": "0.1.0",
  "description": "",
  "main": "src/node.ts",
  "dependencies": {
    "@tensorflow/tfjs": "0.12.4",
    "@tensorflow/tfjs-node": "0.1.11",
    "chatito": "^2.0.0",
    "lodash": "^4.17.10",
    "ts-node": "^7.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.56",
    "@babel/plugin-proposal-class-properties": "^7.0.0-beta.56",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0-beta.56",
    "@babel/plugin-syntax-import-meta": "^7.0.0-beta.56",
    "@babel/preset-env": "^7.0.0-beta.56",
    "@babel/preset-react": "^7.0.0-beta.56",
    "@babel/preset-typescript": "^7.0.0-beta.56",
    "@types/graphql": "^0.13.4",
    "@types/history": "^4.7.0",
    "@types/lodash": "^4.14.116",
    "@types/node": "^10.5.6",
    "@types/react": "^16.4.7",
    "@types/react-dom": "^16.0.6",
    "@types/react-helmet": "^5.0.7",
    "@types/react-router-dom": "^4.3.0",
    "@types/recharts": "^1.0.25",
    "antd": "^3.7.3",
    "axios": "^0.18.0",
    "babel-loader": "^8.0.0-beta.2",
    "babel-plugin-import": "^1.8.0",
    "babel-plugin-styled-components": "^1.5.1",
    "gatsby": "next",
    "gatsby-plugin-react-helmet": "next",
    "gatsby-plugin-styled-components": "next",
    "gatsby-plugin-typescript": "next",
    "gh-pages": "^1.2.0",
    "prettier": "^1.14.0",
    "rc-queue-anim": "^1.6.5",
    "rc-scroll-anim": "^2.5.5",
    "rc-tween-one": "^2.2.3",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-helmet": "^5.2.0",
    "recharts": "^1.1.0",
    "styled-components": "^3.4.0",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.14.0",
    "tslint-plugin-prettier": "^1.3.0",
    "typescript": "3.0.1"
  },
  "scripts": {
    "lint": "npx tslint -c tslint.json 'src/**/*.ts'",
    "cleanup": "rm -rf .cache && find public -not -name models -maxdepth 1 -not -name public -exec rm -rv {} \\;",
    "prettier": "prettier --write '{web,src}/**/*.{ts,tsx}'",
    "gen:dictionary:en": "ts-node src/utils/createDictionaryFromFastext.ts en",
    "gen:dictionary:es": "ts-node src/utils/createDictionaryFromFastext.ts es",
    "gen:dataset:en": "ts-node src/utils/datasetGeneration.ts ./examples/en/aidaConfig.json",
    "gen:dataset:es": "ts-node src/utils/datasetGeneration.ts ./examples/es/aidaConfig.json",
    "web:build": "npm run cleanup && gatsby build",
    "web:start": "npm run cleanup && gatsby develop",
    "web:deploy": "npm run cleanup && gatsby build --prefix-paths && gh-pages -d public",
    "node:start": "mkdir -p ./public/models/pretrained/node/classification && mkdir -p ./public/models/pretrained/node/ner && ts-node src/node.ts",
    "python:convert:classification": "tensorflowjs_converter --input_format=keras ../python/output/trained_classifier.h5 ./public/models/pretrained/keras/classification/.",
    "python:convert:ner": "tensorflowjs_converter --input_format=keras ../python/output/trained_ner.h5 ./public/models/pretrained/keras/ner/.",
    "python:convert": "npm run python:convert:classification && npm run python:convert:ner"
  },
  "author": "Rodrigo Pimentel",
  "license": "BSD-3-Clause"
}
